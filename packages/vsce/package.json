  "scripts": {
    "vscode:prepublish": "npm run getPrebuilds && npm run build:production",
    "build": "webpack --mode development && npm run l10n:export",
    "build:production": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "prebuild": "npm run clean && echo Using TypeScript && tsc --version",
    "clean": "rimraf lib && rimraf dist",
    "watch": "webpack --mode development --watch",
    "test:system": "echo 'Please run the system tests via VSCode UI' && exit 0",
    "test:unit": "jest --config unit.jest_config.ts",
    "test": "npm run test:unit",
    "tsupDep": "node ./scripts/tsup-deps.js",
    "getPrebuilds": "node ./scripts/getSecretsPrebuilds.js",
    "package": "vsce package --dependencies --allow-star-activation && node ../../scripts/mv-pack.js cics-extension-for-zowe vsix",
    "test:e2e:setupExtenderExtension": "cd __tests__/__e2e__/resources/extender-extension && npm install && npm run package && cd ../../../../",
    "test:e2e:setup": "npm run test:e2e:setupExtenderExtension && npm run package && cd __tests__/__e2e__ && docker compose up -d && docker exec code-server bash /config/workspace/setup.sh",
    "test:e2e:teardown": "cd __tests__/__e2e__ && docker exec code-server bash /config/workspace/teardown.sh && docker compose down",
    "test:e2e:run": "npx playwright test -c __tests__/__e2e__/playwright.config.ts --workers=1",
    "test:e2e": "npm run test:e2e:setup && npm run test:e2e:run && npm run test:e2e:teardown",
    "test:e2e:dev:interactive": "npm run test:e2e:setup && npx playwright test -c __tests__/__e2e__/playwright.config.ts --workers=1 --ui && npm run test:e2e:teardown",
    "test:e2e:dev": "npm run test:e2e:setup && npx playwright test -c __tests__/__e2e__/playwright.config.ts --workers=1 --headed && npm run test:e2e:teardown",
    "pretty": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\" --ignore-path ../../.prettierignore",
    "l10n:export": "npx vscode-l10n-dev export \"src/**/*.ts\" --outDir l10n"
  },
